shuru
purno sonkha x = 10;
purno sonkha y = 5;

// Test nested if-else
jodi (x > 5) {
    dekhao "x is greater than 5\n";
    jodi (y > 3) {
        dekhao "y is also greater than 3\n";
        jodi (x > y) {
            dekhao "x is greater than y\n";
        }
        nahoy {
            dekhao "x is not greater than y\n";
        }
    }
    nahoy {
        dekhao "y is not greater than 3\n";
    }
}
nahoy {
    dekhao "x is not greater than 5\n";
}

// Test nested loops
loop (purno sonkha i = 0; i < 3; i++) {
    dekhao "Outer loop iteration: {i}\n";
    loop (purno sonkha j = 0; j < 2; j++) {
        dekhao "  Inner loop iteration: {j}\n";
        jodi (i == j) {
            dekhao "    i equals j\n";
        }
        nahoy {
            dekhao "    i does not equal j\n";
        }
    }
}

ferot dao 0;
shesh
